{
  "name": "carnage/doctrine-encrypted-column",
  "description": "Provides a secure way of transparently encrypting data in doctrine ORM",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Riley"
    }
  ],
  "require": {
    "php": "^7.0",
    "doctrine/orm": "^2.5",
    "ocramius/proxy-manager": "^1.0.2",
    "paragonie/halite": "^2.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit":"^5"
  },
  "autoload": {
    "psr-4": {
      "Carnage\\EncryptedColumn\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Carnage\\EncryptedColumn\\Tests\\": "test/"
    }
  }
}
